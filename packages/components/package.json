{
  "name": "dk-plus",
  "version": "1.0.22",
  "description": "DK-PLUS",
  "keywords": [
    "vue3",
    "DK-PLUS",
    "css",
    "vite",
    "vueJs",
    "components",
    "ui"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./es/index.d.ts"
    },
    "./theme-chalk": "./theme-chalk/",
    "./es": "./es/index.js",
    "./es/*": "./es/*.js",
    "./es/*.js": "./es/*.js",
    "./lib": "./lib/index.js",
    "./lib/*": "./lib/*.js",
    "./lib/*.js": "./lib/*.js",
    "./*": "./*"
  },
  "unpkg": "dist/index.umd.js",
  "style": "dist/index.css",
  "sideEffects": [
    "theme-chalk/index.css",
    "theme-chalk/*",
    "*.css"
  ],
  "author": {
    "name": "dk-plus-ui",
